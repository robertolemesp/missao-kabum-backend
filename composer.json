{
  "name": "kabum/robertos-mission-api",
  "description": "Roberto's Mission - Api",
  "version": "1.0.0",
  "require": {
    "php": ">=8.0",
    "vlucas/phpdotenv": "^5.4",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "monolog/monolog": "^2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Application\\": "src/Application/",
      "Domain\\": "src/Domain/",
      "Infrastructure\\": "src/Infrastructure/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "composer dump-autoload"
    ]
  }
}
